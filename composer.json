{
    "name": "alexminza/victoriabank-sdk",
    "description": "PHP SDK for Victoriabank API",
    "type": "library",
    "keywords": [
        "victoriabank",
        "moldova",
        "payments",
        "visa",
        "mastercard",
        "api",
        "sdk"
    ],
    "homepage": "https://github.com/alexminza/victoriabank-sdk-php",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Alexander Minza",
            "homepage": "https://www.alexminza.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/alexminza/victoriabank-sdk-php/issues",
        "forum": "https://github.com/alexminza/victoriabank-sdk-php/discussions",
        "source": "https://github.com/alexminza/victoriabank-sdk-php"
    },
    "require": {
        "php": ">=7.2.5",
        "guzzlehttp/guzzle": "^7.0",
        "guzzlehttp/guzzle-services": "^1.0",
        "firebase/php-jwt": "^6.0"
    },
    "suggest": {
        "monolog/monolog": "To enable logging of HTTP requests and responses."
    },
    "autoload": {
        "psr-4": {
            "Victoriabank\\": "src/"
        }
    },
    "require-dev": {
        "vlucas/phpdotenv": "*",
        "phpunit/phpunit": "*",
        "monolog/monolog": "*"
    },
    "config": {
        "platform": {
            "php": "7.2.5"
        }
    }
}